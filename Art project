let Ellipse = 50
function setup() {
  createCanvas(400, 400);
  background(220);

  
}


  
// Choosing colors//  

function draw() {
  
  for(var i = 0;i<10;i++){
    for(var j = 0;j<10;j++){
      let point1X = i*Ellipse
      let point1Y = j*Ellipse
      
ellipse(point1X, point1Y, 50,50)

      let fillcolorred = random(100,255)
      let fillcolorgreen = random(0)
        let fillcolorblue = random(100,190)
         fill( fillcolorred,fillcolorgreen,fillcolorblue)
    }
  }
}
